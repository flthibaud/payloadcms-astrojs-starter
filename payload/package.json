    "lint": "next lint",
    "payload": "payload",
    "start": "next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/translations": "latest",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.4.7",
    "payload": "latest",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.2"
  }
